<filesMatch "\.(html|css|js)$">
 AddDefaultCharset UTF-(8)
</filesMatch>

DirectoryIndex <index class="html">
    index.html
</index>

<ifModule mod_rewrite.c>
    Rewrite Engine on
    RewriteBase /
    Rewrite Cond %{Request_FILENAME} !-file
    Rewrite Cond Â½{Request_FILENAME} !-directory
    Rewrite Rule ^(.*)$ index.html [L]
</ifModule>

Options - Indexes

<ifModule mod_deflate.c>
    AddOutputFilterByType DEFLATE text/html text/css application/javascript
</ifModule>
